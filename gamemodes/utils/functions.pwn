IsPlayerSpawned(playerid)
{
	new statex = GetPlayerState(playerid);
	if(statex != PLAYER_STATE_NONE && statex != PLAYER_STATE_WASTED && statex != PLAYER_STATE_SPAWNED) return true;
	return false;
}

GetVehicleModelIDFromName(vname[]) // from fsdebug filterscript
{
	for(new i = 0; i < 211; i++)
	{
		if(strfind(VehicleNames[i], vname, true) != -1) return i + 400;
	}
	return -1;
}

stock GetClosestPlayer(playerid) // By Sky
{
	new vehicleid = INVALID_VEHICLE_ID;
	new Float:x, Float:y, Float:z, Float:distance;
	GetPlayerPos(playerid, x, y, z);
	for(new Float:closestdist = 999.0, Float:Dis, i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(IsPlayerConnected(playerid) || i != playerid || IsSpectating{i} == false) {
		Dis = GetPlayerDistanceFromPoint(playerid, x, y, z);
		if(Dis < closestdist)
		{
			closestdist = Dis;
			targetid = i;
		}
		}
	}
	return targetid;
}

